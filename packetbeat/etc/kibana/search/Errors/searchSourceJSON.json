{
    "filter": [
        {
            "meta": {
                "disabled": false,
                "index": "packetbeat-*",
                "key": "status",
                "negate": true,
                "value": "OK"
            },
            "query": {
                "match": {
                    "status": {
                        "query": "OK",
                        "type": "phrase"
                    }
                }
            }
        }
    ],
    "highlight": {
        "fields": {
            "*": {}
        },
        "post_tags": [
            "@/kibana-highlighted-field@"
        ],
        "pre_tags": [
            "@kibana-highlighted-field@"
        ]
    },
    "index": "packetbeat-*",
    "query": {
        "query_string": {
            "analyze_wildcard": true,
            "query": "*"
        }
    }
}