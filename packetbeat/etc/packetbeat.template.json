              "index": true,
              "type": "keyword"
            },
            "match_mapping_type": "string"
          }
        }
      ],
      "properties": {
        "@timestamp": {
          "type": "date"
        },
        "client_location": {
          "type": "geo_point"
        },
        "dest": {
          "properties": {
            "ip4_location": {
              "type": "geo_point"
            },
            "ip6_location": {
              "type": "geo_point"
            },
            "outer_ip4_location": {
              "type": "geo_point"
            },
            "outer_ip6_location": {
              "type": "geo_point"
            }
          }
        },
        "last_time": {
          "type": "date"
        },
        "params": {
          "norms": false,
          "type": "text"
        },
        "query": {
          "ignore_above": 0,
          "type": "keyword"
        },
        "request": {
          "norms": false,
          "type": "text"
        },
        "response": {
          "norms": false,
          "type": "text"
        },
        "source": {
          "properties": {
            "ip4_location": {
              "type": "geo_point"
            },
            "ip6_location": {
              "type": "geo_point"
