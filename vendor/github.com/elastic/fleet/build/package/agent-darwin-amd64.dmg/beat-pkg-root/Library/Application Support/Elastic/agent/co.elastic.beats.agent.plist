<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
        <string>co.elastic.beats.agent</string>
	<key>ProgramArguments</key>
	<array>
            <string>/Library/Application Support/Elastic/agent/bin/agent</string>
            <string>-c</string>
            <string>/etc/agent/agent.yml</string>
            <string>--path.home</string>
            <string>/Library/Application Support/Elastic/agent</string>
            <string>--path.config</string>
            <string>/etc/agent</string>
            <string>--path.data</string>
            <string>/var/lib/agent</string>
            <string>--path.logs</string>
            <string>/var/log/agent</string>
        </array>
	<key>RunAtLoad</key>
        <true/>
</dict>
</plist>
