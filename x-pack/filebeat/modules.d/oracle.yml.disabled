# Module: oracle
# Docs: https://www.elastic.co/guide/en/beats/filebeat/8.14/filebeat-module-oracle.html

- module: oracle
  database_audit:
    enabled: false

    # Set which input to use between syslog or file (default).
    #var.input: file

    # Set paths for the log files when file input is used.
    # Should only be used together with file input
    #var.paths: ["/home/user/oracleauditlogs/*.aud"]
