# Module: zeek
# Docs: https://www.elastic.co/guide/en/beats/filebeat/master/filebeat-module-zeek.html

- module: zeek
  # All logs
  connection:
    enabled: true
  dns: 
    enabled: true
  http:
    enabled: true
  files:
    enabled: true
  ssl:
    enabled: true
  notice:
    enabled: true

    # Set custom paths for the log files. If left empty,
    # Filebeat will choose the paths depending on your OS.
    #var.paths:
