{
  "description": "Pipeline for parsing azure activity logs.",
  "processors": [
      {
          "date": {
              "field": "azure.activitylogs.time",
              "target_field": "@timestamp",
              "ignore_failure": false,
              "formats": [
                  "ISO8601"
              ]
          }
      },
      {
          "rename": {
              "field": "azure.activitylogs.resourceId",
              "target_field": "azure.activitylogs.resourceID",
              "ignore_missing": true
          }
      },
      {
          "remove": {
              "field": ["message", "azure.activitylogs.time"],
              "ignore_missing": true
          }
      },
      {
          "drop": {
              "if" : "ctx.azure.activitylogs.identity == null || ctx.azure.activitylogs.identity.claims == null"
          }
      }
  ],
  "on_failure": [
  {
    "set": {
      "field": "error.message",
      "value": "{{ _ingest.on_failure_message }}"
    }
  }
  ]
}
