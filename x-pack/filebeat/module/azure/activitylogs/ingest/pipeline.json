{
  "description": "Pipeline for parsing azure activity logs.",
  "processors": [
    {
      "remove": {
        "field":"message",
        "ignore_missing": true
      }
    }
  ],
  "on_failure": [
  {
    "set": {
      "field": "error.message",
      "value": "{{ _ingest.on_failure_message }}"
    }
  }
  ]
}
