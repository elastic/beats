{
  "description": "Pipeline for normalizing Kubernetes coredns logs",
  "processors": [
    {
      "script": {
        "lang": "painless",
        "source": "ctx.event.created = ctx['@timestamp']; ctx['@timestamp'] = ctx['coredns']['timestamp']; ctx.coredns.remove('timestamp');",
        "ignore_failure" : true
      }
    },
    {
      "set": {
        "field": "source.ip",
        "value": "{{coredns.client}}"
      }
    }
  ]
}
