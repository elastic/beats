{
  "description": "Pipeline for normalizing Zeek http.log",
  "processors": [
    {
      "set": {
        "field": "event.created",
        "value": "{{_ingest.timestamp}}"
      }
    },
    {
      "date": {
        "field": "zeek.http.ts",
        "formats": ["UNIX"]
      }
    },
    {
      "remove": {
        "field": "zeek.http.ts"
      }
    },
    {
      "geoip": {
        "field": "destination.ip",
        "target_field": "destination.geo"
      }
    },
    {
      "geoip": {
        "field": "source.ip",
        "target_field": "source.geo"
      }
    },
    {
      "user_agent": {
        "field": "zeek.http.request.user_agent.raw",
        "target_field": "zeek.http.request.user_agent",
        "ignore_missing": true
      }
    }
  ],
  "on_failure" : [{
    "set" : {
      "field" : "error.message",
      "value" : "{{ _ingest.on_failure_message }}"
    }
  }]
}
