{
  "description": "Pipeline for normalizing Zeek ntlm.log",
  "processors": [
    {
      "set": {
        "field": "event.created",
        "value": "{{_ingest.timestamp}}"
      }
    },
    {
      "date": {
        "field": "zeek.ntlm.ts",
        "formats": ["UNIX"]
      }
    },
    {
      "remove": {
        "field": "zeek.ntlm.ts"
      }
    }
  ],
  "on_failure" : [{
    "set" : {
      "field" : "error.message",
      "value" : "{{ _ingest.on_failure_message }}"
    }
  }]
}
