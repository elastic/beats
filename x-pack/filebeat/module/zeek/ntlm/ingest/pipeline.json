{
  "description": "Pipeline for normalizing Zeek ntlm.log",
  "processors": [
    {
      "date": {
        "field": "@timestamp",
        "target_field": "event.created",
        "formats": ["ISO8601"]
      }
    },
    {
      "date": {
        "field": "zeek.ntlm.ts",
        "formats": ["UNIX"]
      }
    },
    {
      "remove": {
        "field": "zeek.ntlm.ts"
      }
    }
  ],
  "on_failure" : [{ 
    "set" : { 
      "field" : "error.message", 
      "value" : "{{ _ingest.on_failure_message }}" 
    } 
  }]
}
