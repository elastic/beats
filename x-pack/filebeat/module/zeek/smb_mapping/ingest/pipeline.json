{
  "description": "Pipeline for normalizing Zeek smb_mapping.log",
  "processors": [
    {
      "set": {
        "field": "event.created",
        "value": "{{_ingest.timestamp}}"
      }
    },
    {
      "date": {
        "field": "zeek.smb_mapping.ts",
        "formats": ["UNIX"]
      }
    },
    {
      "remove": {
        "field": "zeek.smb_mapping.ts"
      }
    }
  ],
  "on_failure": [{
    "set": {
      "field": "error.message",
      "value": "{{ _ingest.on_failure_message }}"
    }
  }]
}
