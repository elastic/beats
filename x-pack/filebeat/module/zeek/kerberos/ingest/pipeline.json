{
  "description": "Pipeline for normalizing Zeek kerberos.log",
  "processors": [
    {
      "script": {
        "lang": "painless",
        "source": "ctx.event.created = ctx['@timestamp']; ctx['@timestamp'] = (long)ctx['zeek']['kerberos']['ts'] * 1000; ctx.zeek.kerberos.remove('ts');"
      }
    },
    {
      "set": {
        "field": "event.id",
        "value": "{{zeek.session_id}}",
        "if": "ctx.zeek.session_id != null"
      }
    },
    {
      "set": {
        "field": "source.ip",
        "value": "{{source.address}}"
      }
    },
    {
      "set": {
        "field": "destination.ip",
        "value": "{{destination.address}}"
      }
    },
    {
      "date": {
        "field": "zeek.kerberos.valid_until",
        "target_field": "zeek.kerberos.valid_until",
        "formats": ["UNIX"],
        "if": "ctx.zeek.kerberos.valid_until != null"
      }
    }
  ],
  "on_failure": [{
    "set": {
      "field": "error.message",
      "value": "{{ _ingest.on_failure_message }}"
    }
  }]
}
