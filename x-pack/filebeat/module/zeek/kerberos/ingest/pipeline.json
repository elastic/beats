{
  "description": "Pipeline for normalizing Zeek kerberos.log",
  "processors": [
    {
      "date": {
        "field": "@timestamp",
        "target_field": "event.created",
        "formats": ["ISO8601"]
      }
    },
    {
      "date": {
        "field": "zeek.kerberos.ts",
        "formats": ["UNIX"]
      }
    },
    {
      "remove": {
        "field": "zeek.kerberos.ts"
      }
    },
    {
      "date": {
        "field": "zeek.kerberos.valid_until",
        "target_field": "zeek.kerberos.valid_until",
        "formats": ["UNIX"],
        "if": "ctx.zeek.kerberos.valid_until != null"
      }
    }
  ],
  "on_failure": [{
    "set": {
      "field": "error.message",
      "value": "{{ _ingest.on_failure_message }}"
    }
  }]
}
