{
  "description": "Pipeline for normalizing Zeek dns.log",
  "processors": [
    {
      "set": {
        "field": "event.created",
        "value": "{{_ingest.timestamp}}"
      }
    },
    {
      "date": {
        "field": "zeek.dns.ts",
        "formats": ["UNIX"]
      }
    },
    {
      "remove": {
        "field": "zeek.dns.ts"
      }
    },
    {
      "foreach": {
        "field": "zeek.dns.answers",
        "if": "ctx.zeek.dns.response_code?.name == \"NOERROR\" && ctx.zeek.dns.query?.class?.name == \"C_INTERNET\"",
        "ignore_missing": true,
        "processor": {
          "geoip": {
            "field": "_ingest._value",
            "target_field": "zeek.dns.geo",
            "ignore_failure": true
          }
        }
      }
    }
  ],
  "on_failure" : [{
    "set" : {
      "field" : "error.message",
      "value" : "{{ _ingest.on_failure_message }}"
    }
  }]
}
