# Module: oracle
# Docs: https://www.elastic.co/guide/en/beats/metricbeat/main/metricbeat-module-oracle.html

- module: oracle
  period: 10m
  metricsets:
    - tablespace
  enabled: true
  hosts: ["user/pass@0.0.0.0:1521/ORCLPDB1.localdomain"]
- module: oracle
  period: 10s
  metricsets:
    - performance
  enabled: true
  hosts: ["user/pass@0.0.0.0:1521/ORCLPDB1.localdomain"]
- module: oracle
  period: 60s
  metricsets:
    - sysmetric
  enabled: true
  # patterns: ["foo%","%bar","%foobar%"]
  hosts: ["user/pass@0.0.0.0:1521/ORCLPDB1.localdomain"]
  
  # username: ""
  # password: ""

