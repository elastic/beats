{
  "description": "Pipeline for parsing elasticsearch audit logs",
  "processors": [
      {
          "rename": {
              "field": "@timestamp",
              "target_field": "event.created"
          }
      },
      {
          "grok": {
              "field": "message",
              "patterns": [
                  "\\[%{TIMESTAMP_ISO8601:elasticsearch.audit.timestamp}\\]%{SPACE}*(\\[%{WORD:elasticsearch.node.name}\\])?%{SPACE}*\\[%{WORD:elasticsearch.audit.layer}\\]%{SPACE}*\\[%{WORD:elasticsearch.audit.event_type}\\]%{SPACE}*(origin_type\\=\\[%{WORD:elasticsearch.audit.origin_type}\\])?,?%{SPACE}*(origin_address\\=\\[%{IPORHOST:elasticsearch.audit.origin_address}\\])?,?%{SPACE}*(principal\\=\\[%{WORD:elasticsearch.audit.principal}\\])?,?%{SPACE}*(action\\=\\[%{DATA:elasticsearch.audit.action}\\])?,?%{SPACE}*?(uri=\\[%{DATA:elasticsearch.audit.uri}\\])?,?%{SPACE}*(request\\=\\[%{WORD:elasticsearch.audit.request}\\])?,?%{SPACE}*(request_body\\=\\[%{DATA:elasticsearch.audit.request_body}\\])?,?"
              ]
          }
      },
      {
          "rename": {
              "field": "elasticsearch.audit.timestamp",
              "target_field": "@timestamp"
          }
      }
    ],
  "on_failure" : [{
    "set" : {
      "field" : "error.message",
      "value" : "{{ _ingest.on_failure_message }}"
    }
  }]
}
