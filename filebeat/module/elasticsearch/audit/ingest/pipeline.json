{
  "description": "Pipeline for parsing elasticsearch audit logs",
  "processors": [
      {
          "rename": {
              "field": "@timestamp",
              "target_field": "event.created"
          }
      },
      {
          "grok": {
              "field": "message",
              "patterns": [
                  "\\[%{TIMESTAMP_ISO8601:timestamp}\\]\\s*\\[%{WORD:layer}\\]\\s*\\[%{WORD:event_type}\\]\\s*(origin_type\\=\\[%{WORD:origin_type}\\])?,?\\s*(origin_address\\=\\[%{IPORHOST:origin_address}\\])?,?\\s*(principal\\=\\[%{WORD:principal}\\])?,?\\s*(action\\=\\[%{DATA:action}\\])?,?\\s*?(uri=\\[%{DATA:uri}\\])?,?\\s*(request\\=\\[%{WORD:request}\\])?,?"
              ]
          }
      },
      {
          "rename": {
              "field": "timestamp",
              "target_field": "@timestamp"
          }
      }
    ],
  "on_failure" : [{
    "set" : {
      "field" : "error.message",
      "value" : "{{ _ingest.on_failure_message }}"
    }
  }]
}
