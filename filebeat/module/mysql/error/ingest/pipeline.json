{
  "description": "Pipeline for parsing MySQL error logs",
  "processors": [{
    "grok": {
        "field": "message",
        "patterns": [
          "%{LOCALDATETIME:mysql.error.timestamp} (\\[%{DATA:log.level}\\] )?%{GREEDYDATA:message}",
          "%{DATA:mysql.error.timestamp} %{NUMBER:mysql.thread_id:long} \\[%{DATA:log.level}\\] %{GREEDYDATA:message}",
          "%{GREEDYDATA:message}"
        ],
        "ignore_missing": true,
        "pattern_definitions": {
          "LOCALDATETIME": "[0-9]+ %{TIME}"
        }
      }
    }, {
        "rename": {
            "field": "@timestamp",
            "target_field": "event.created"
        }
    }, {
      "date": {
        "field": "mysql.error.timestamp",
        "target_field": "@timestamp",
        "formats": [
          "ISO8601",
          "yyMMdd H:m:s"
        ],
        "ignore_failure": true
      }
    }, {
      "remove":{
        "field": "mysql.error.timestamp",
        "ignore_missing": true
      }
    }],
  "on_failure" : [{
    "set" : {
      "field" : "error.message",
      "value" : "{{ _ingest.on_failure_message }}"
    }
  }]
}
