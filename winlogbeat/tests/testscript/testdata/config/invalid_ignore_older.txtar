[!windows] skip 'requires Windows'

! exec winlogbeat test config -c winlogbeat.yml
stderr 'unknown unit " hour" in duration'

-- winlogbeat.yml --
winlogbeat.event_logs:
  - name: Application
    ignore_older: 1 hour
output.file:
  path: ${WORK:?missing WORK}/output
  filename: winlogbeat
  rotate_every_kb: 1000
