{
  "mappings": {
    "_default_": {
      "_all": {
        "norms": false
      },
      "dynamic_templates": [
        {
          "fields": {
            "mapping": {
              "ignore_above": 1024,
              "index": true,
              "type": "keyword"
            },
            "match_mapping_type": "string",
            "path_match": "fields.*"
          }
        }
      ],
      "properties": {
        "@timestamp": {
          "type": "date"
        },
        "beat": {
          "properties": {
            "hostname": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "name": {
              "ignore_above": 1024,
              "type": "keyword"
            }
          }
        },
        "cpu": {
          "properties": {
            "system_p": {
              "type": "float"
            },
            "user_p": {
              "type": "float"
            }
          }
        },
        "cpus": {
          "properties": {
            "cpuX": {
              "properties": {
                "system_p": {
                  "type": "float"
                },
                "user_p": {
                  "type": "float"
                }
              }
            }
          }
        },
        "fs": {
          "properties": {
            "device_name": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "mount_point": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "used_p": {
              "type": "float"
            }
          }
        },
        "load": {
          "properties": {
            "load1": {
              "type": "float"
            },
            "load15": {
              "type": "float"
            },
            "load5": {
              "type": "float"
            }
          }
        },
        "mem": {
          "properties": {
            "actual_used_p": {
              "type": "float"
            },
            "used_p": {
              "type": "float"
            }
          }
        },
        "proc": {
          "properties": {
            "cmdline": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "cpu": {
              "properties": {
                "start_time": {
                  "ignore_above": 1024,
                  "type": "keyword"
                },
                "total_p": {
                  "type": "float"
                }
              }
            },
            "mem": {
              "properties": {
                "rss_p": {
                  "type": "float"
                }
              }
            },
            "name": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "state": {
              "ignore_above": 1024,
              "type": "keyword"
            },
            "username": {
              "ignore_above": 1024,
              "type": "keyword"
            }
          }
        },
        "swap": {
          "properties": {
            "used_p": {
              "type": "float"
            }
          }
        },
        "tags": {
          "ignore_above": 1024,
          "type": "keyword"
        },
        "type": {
          "ignore_above": 1024,
          "type": "keyword"
        }
      }
    }
  },
  "order": 0,
  "settings": {
    "index.refresh_interval": "5s"
  },
  "template": "topbeat-*"
}